{
  "root": true,
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "plugins": ["react"],
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "react/jsx-indent": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "no-console": ["warn", { "allow": ["warn", "error", "info"] }]
  }
}
